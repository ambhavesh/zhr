{
  "name": "ztimetracker",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.7",
    "@cap-js/hana": "^2.3.3",
    "@sap-cloud-sdk/connectivity": "^4",
    "@sap-cloud-sdk/http-client": "^4",
    "@sap-cloud-sdk/resilience": "^4",
    "@sap/cds": "^9.4.4",
    "@sap/ui5-builder-webide-extension": "^1.1.9",
    "@sap/ux-ui5-tooling": "^1.19.6",
    "@sap/xssec": "^4",
    "@ui5/cli": "^4.0.38",
    "express": "^4",
    "nodemailer": "^7.0.11",
    "qrcode": "^1.5.4",
    "ui5-cc-spreadsheetimporter": "^2.4.0",
    "ui5-task-zipper": "^3.5.0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "cds-plugin-ui5": "^0.13.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "h": "cds watch --profile hybrid",
    "watch-zmngemployeepunching": "cds watch --open zmngemployeepunching/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy ztimetracker --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "d": "cf deploy mta_archives/ztimetracker_1.0.0.mtar",
    "watch-zpunchinout": "cds watch --open zpunchinout/webapp/index.html?sap-ui-xx-viewCache=false",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-zcreateleaverequest": "cds watch --open zcreateleaverequest/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-zleaverequest": "cds watch --open zleaverequest/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-project1": "cds watch --open project1/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-zleaveinbox": "cds watch --open zleaveinbox/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-employee": "cds watch --open employee/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "server": {
      "index": true
    },
    "requires": {
      "auth": "dummy",
      "db": "hana"
    },
    "sql": {
      "native_hana_associations": false
    }
  },
  "sapux": [
    "app/zmngemployeepunching",
    "app/zcreateleaverequest",
    "app/project1"
  ],
  "workspaces": [
    "app/*"
  ]
}
